{
  "manifest_version": 2,
  "short_name": "Morse",
  "name": "M³ Translator",
  "description": "Possible to Morse (or Braille) ⇄ English, Korean, Japanese or etc!",
  "version": "3.6.0",
  "app": {
    "urls": [
      "https://jinh.kr/braille/"
    ],
    "launch": {
      "web_url": "https://jinh.kr/braille/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "icons": [{
    "src": "image/favicon_b.ico",
    "type": "image/ico",
    "sizes": "16x16"
  }, {
    "src": "image/icon_b.webp",
    "type": "image/webp",
    "sizes": "512x512"
  }],
  "start_url": "index.html",
  "background_color": "#f44336",
  "display": "standalone",
  "orientation": "portrait",
  "scope": "/braille/",
  "theme_color": "#f44336"
  ,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.morsecode.translator.jinh",
      "id": "com.morsecode.translator.jinh"
    }
  ]
}
